    #right
        %table{:border => 1, :width => "70%", :class => "project_table"}
            %tr{:height => "10px"}
                %th{:align=> "left"}
                    = link_to "Project Name", unapproved_completed_projects_index_path(sort_params params, :name)
                %th{:align=> "left"} Description
                %th{:align=> "left"} Status
                %th{:align=> "left"} Tags
                %th{:align=> "left"}
                    = link_to "Agency", unapproved_completed_projects_index_path(sort_params params,  :agency)
                %th{:align=> "left"}
                    = link_to "Date", unapproved_completed_projects_index_path(sort_params params,  :date)
                %th{:align=> "left"} Approve Completion
        
            - ProjectPresenter.all(@projects) do |project|
                %tr{:height => "10px"}
                    %td= project.profile
                    %td= project.description
                    %td= project.status
                    %td= project.tags
                    %td= project.agency_profile
                    %td= project.date
                    %td= button_to 'Approve', approve_completed_project_path(project.id)
    #footer
