#body.blended_grid
  #left.pageLeftMenu
    %input{:size => "15", :maxlength => "100", :name => "search_index", :type => "text", :value => ""}/
    %br
    %button{:type => "button"} Search
    %p
    = link_to 'All projects', projects_path
    %br
    = link_to 'All TAMU users', tamu_users_path
    %br
    = link_to 'All Agencies', agencies_path
    %br
  #right.pageContent
    %table{:border => 1, :width => "70%"}
      %tr{:height => "10px"}
        %th{:align=> "left", :width => "50%"} Project Name
        %th{:align=> "left", :width => "20%"} Post Date
        %th{:align=> "left", :width => "30%"} Organization
      - @projects.each do |project|
        %tr{:height => "10px"}
          %td= link_to project.name, project_path(project)
          %td= project.created_at.to_date
          %td= project.agency
#footer.pageFooter

