%html
  %head
    %title TAMU Non-Profit Projects Collect System
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    #maroon_top
      %h1.title TAMU Non-Profit Projects Collect System
      .top-header
        #a-maroon
          = link_to 'Home', root_path
          
          = link_to "My page", mypage_path
          
          = link_to 'All TAMU Users', tamu_users_path
         
          = link_to 'All Agencies', agencies_path

          = link_to 'All Projects', projects_path

          - if logged_in?
            = link_to "Log out", logout_path
          - else
            = link_to 'Login', login_path
          
      / .top-header
      #login-as
        - if logged_in?
          Logged in as #{current_user.name}
    #main
      #layoutcenter
        - if flash[:notice]
          #notice.message!= flash[:notice]
        - elsif flash[:warning]
          #warning.message!= flash[:warning]

        = yield
    #maroon_footer
      test
    -#= debug(params) if Rails.env.development?
    -#%br
    -#= debug(session) if Rails.env.development?
