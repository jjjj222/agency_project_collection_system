%h1 All Agencies (TODO)
%body
    #content
        #left{:float => "left", :width => "30%"}
            -#%p
            -#= link_to "My page", agency_path(1)
            -#%br
            -#%br
        #right
            %table{:border => 1, :width => "70%"}
                %tr{:height => "10px"}
                    %th{:align=> "left"} Name
                    - if @current_user.class.name == "TamuUser" and @current_user.admin?
                        %th{:align=> "left"} Unapprove Agency
            
                - @agencies.each do |agency|
                    %tr{:height => "10px"}
                        %td= link_to agency.name, agency_path(agency.id)
                        - if @current_user.class.name == "TamuUser" and @current_user.admin?
                            %td= button_to 'Unapprove Agency', unapprove_agency_path(agency)
        #footer
